  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ReqIFSharp" Version="5.0.0-rc1" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\images\" />
  </ItemGroup>

</Project>
